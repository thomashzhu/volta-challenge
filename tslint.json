{
  "extends": "gts/tslint.json",
  "rules": {
    "class-name": false,
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape",
      "avoid-template"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case",
      "ban-keywords",
      "check-format"
    ]
  }
}
