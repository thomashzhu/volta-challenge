{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2015"
    ],
    "skipLibCheck": true
  },
  "include": [
    "src/*.ts",
    "src/**/*.ts",
    "test/*.ts",
    "test/**/*.ts"
  ],
  "files": [
    "custom.d.ts"
  ]
}
